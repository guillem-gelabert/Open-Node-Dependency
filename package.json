{
  "name": "open-node-dependency",
  "displayName": "Open Node Dependency",
  "description": "Navigate to the package.json of a dependency in node_modules",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "keywords": [
    "package.json",
    "dependencies",
    "node_modules",
    "navigation"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "open-node-dependency.toggle",
        "title": "Toggle Package.json Navigation"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@types/node": "22.x",
    "@types/vscode": "^1.105.0",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  }
}
